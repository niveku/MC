     "input": [
      "%pylab inline\n",
      "x=linspace(0,2*pi,5)\n",
      "y=sin(x)\n",
      "figure()\n",
      "plot (x,y,\"o\")\n",
      "xlim(-1, 8)\n",
      "ylim(-1.5,1.5)\n",
      "inter = interpolate.splev(xnew, tck, der=0)\n",
      "generar = poly1d (inter)\n",
      "xn = linspace (0,2*pi,50)\n",
      "plot (xn, generar(xn), \"--\", color = \"red\", label = \"linear\")\n",
      "show()"
     ],
